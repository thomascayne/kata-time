{
  "name": "ts-kata",
  "version": "1.0.0",
  "description": "Test Development Design Playground with Typescript + Jest + Kata for Thomas Cayne",
  "main": "index.js",
  "scripts": {
    "start": "jest --",
    "watch": "jest --watch --",
    "watch-all": "jest --watchAll"
  },
  "author": "thomas.cayne@slashand.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "prettier": "2.0.2",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@types/jest": "25.1.4",
    "acorn": ">=7.1.1",
    "babel-jest": "25.2.4",
    "handlebars": ">=4.7.3",
    "jest": "25.2.4",
    "lodash": ">=4.17.19",
    "minimist": ">=1.2.5",
    "rxjs": "6.5.4",
    "typescript": "3.8.3"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/**/*.spec.ts",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
