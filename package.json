{
  "name": "ts-kata",
  "version": "1.0.0",
  "description": "Test Development Design Playground with Typescript + Jest + Kata for Thomas Cayne",
  "main": "index.js",
  "scripts": {
    "start": "jest --",
    "watch": "jest --watch --",
    "watch-all": "jest --watchAll"
  },
  "author": "thomas.cayne@slashand.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "prettier": "1.17.1",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.13",
    "acorn": ">=5.7.4",
    "babel-jest": "24.8.0",
    "handlebars": ">=4.3.0",
    "jest": "24.8.0",
    "lodash": ">=4.17.12",
    "minimist": ">=1.2.2",
    "rxjs": "6.5.2",
    "typescript": "3.5.1"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/**/*.spec.ts",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
